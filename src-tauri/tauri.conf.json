{
  "productName": "ATM",
  "version": "0.3.0",
  "identifier": "com.cubezhao.atm",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ATM",
        "width": 800,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 700
      }
    ],
    "security": {
      "csp": "default-src 'self'; connect-src ipc: http://ipc.localhost https://oauth.augmentcode.com https://portal.withorb.com https://aug.zhinianblog.cn https://login.microsoftonline.com https://outlook.office365.com https://* http://*; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'"
    }
  },
  "bundle": {
    "active": true,
    "targets": ["nsis", "msi", "dmg", "app"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ]
  }
}
